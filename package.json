{
  "name": "generator-rv",
  "version": "0.0.1",
  "description": "Scaffold out a node module for vue component",
  "main": "dist/index.js",
  "unpkg": "dist/index.min.js",
  "module": "dist/index.esm.js",
  "scripts": {
    "build": "npm run build:dev && npm run build:prod && npm run build:esm",
    "build:dev": "rollup -c rollup.config.dev.js",
    "build:prod": "rollup -c rollup.config.prod.js",
    "build:esm": "rollup -c rollup.config.esm.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dangvanthanh/generator-rv.git"
  },
  "author": "Dang Van Thanh <chieuhue@gmail.com> (dangthanh.org)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dangvanthanh/generator-rv/issues"
  },
  "homepage": "https://github.com/dangvanthanh/generator-rv#readme",
  "devDependencies": {
    "rollup": "^0.57.0",
    "rollup-plugin-alias": "^1.4.0",
    "rollup-plugin-buble": "^0.19.2",
    "rollup-plugin-commonjs": "^9.1.0",
    "rollup-plugin-node-resolve": "^3.2.0",
    "rollup-plugin-uglify": "^3.0.0",
    "rollup-plugin-vue": "^3.0.0",
    "uglify-es": "^3.3.9",
    "vue-template-compiler": "^2.5.16"
  }
}
